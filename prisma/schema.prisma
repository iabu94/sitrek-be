  params                                                       String                          @db.Text
  lastResetTime                                                DateTime                        @default(dbgenerated("'0000-00-00 00:00:00'")) @db.DateTime(0)
  resetCount                                                   Int                             @default(0)
  otpKey                                                       String                          @default("") @db.VarChar(1000)
